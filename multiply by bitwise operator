// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int mul(int num1, int num2)
{
    int result=0;
   while(num2>0)
   {
       if(num2&1)
       result+=num1;
       
       num2>>=1;
       num1<<=1;
   }
   return result;
}

int main() {
    int a=16,b=8;
    cout<<mul(a,b);



    return 0;
}
