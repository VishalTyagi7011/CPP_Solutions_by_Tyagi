// Online C++ compiler to run C++ program online
#include <iostream>


int main() {
int arr[]={2,4,6,8,10,12,14,16},size=sizeof(arr)/sizeof(arr[0]),rotate=4;

for(int i=0;i<size;i++)
{
    if(i<size-rotate)
    std::cout<<arr[i+rotate]<<" ";
    else
    std::cout<<(arr[i+rotate-size])<<" ";
}


    return 0;
}
